{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "FileCreate": {
          "@onmatch": "exclude",
          "Rule": {
            "@groupRelation": "and",
            "Image": {
              "@condition": "image",
              "#text": "Outlook.exe"
            },
            "TargetFilename": {
              "@condition": "contains",
              "#text": "Roaming\\Microsoft\\Outlook\\Outlook.xml"
            }
          }
        }
      }
    }
  }
}