{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "include",
          "TargetObject": [
            {
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Authentication\\Credential Provider"
            },
            {
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Authentication\\Plap Providers"
            },
            {
              "@condition": "begin with",
              "#text": "HKLM\\SYSTEM\\CurrentControlSet\\Control\\Lsa"
            },
            {
              "@name": "technique_id=T1003,technique_name=Credential Dumping",
              "@condition": "begin with",
              "#text": "HKLM\\SYSTEM\\CurrentControlSet\\Control\\SecurityProviders\\"
            },
            {
              "@name": "technique_id=T1003,technique_name=Credential Dumping",
              "@condition": "contains",
              "#text": "\\Control\\SecurityProviders\\WDigest"
            }
          ]
        }
      }
    }
  }
}