{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "exclude",
          "TargetObject": [
            {
              "@condition": "contains",
              "#text": "\\{CAFEEFAC-"
            },
            {
              "@condition": "begin with",
              "#text": "HKLM\\COMPONENTS"
            }
          ],
          "EventType": {
            "@condition": "is",
            "#text": "CreateKey"
          }
        }
      }
    }
  }
}