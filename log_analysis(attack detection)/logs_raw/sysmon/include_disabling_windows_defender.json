{
  "Sysmon": {
    "@schemaversion": "4.70",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "ProcessCreate": {
          "@onmatch": "include",
          "Rule": {
            "@groupRelation": "and",
            "CommandLine": [
              {
                "@condition": "contains",
                "#text": "Set-MpPreference"
              },
              {
                "@condition": "contains any",
                "#text": "-DisableRealTimeMonitoring $true;-DisableBehaviorMonitoring $true;-DisableBlockAtFirstSeen $true;-DisableIOAVProtection $true;-DisablePrivacyMode $true;-SignatureDisableUpdateOnStartupWithoutEngine $true;-DisableArchiveScanning $true;-DisableIntrusionPreventionSystem $true;-DisableScriptScanning $true"
              }
            ]
          }
        }
      }
    }
  }
}