{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "include",
          "TargetObject": [
            {
              "@name": "technique_id=T1562.002,technique_name=Disable Windows Event Logging",
              "@condition": "contains all",
              "#text": "\\SYSTEM\\;\\Service\\EventLog;Retention"
            },
            {
              "@name": "technique_id=T1562.002,technique_name=Disable Windows Event Logging",
              "@condition": "contains all",
              "#text": "\\SYSTEM\\;\\Service\\EventLog;MaxSize"
            }
          ]
        }
      }
    }
  }
}