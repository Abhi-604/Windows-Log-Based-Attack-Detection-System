{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "exclude",
          "Rule": {
            "@groupRelation": "and",
            "Image": {
              "@condition": "image",
              "#text": "svchost.exe"
            },
            "TargetObject": {
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Schedule\\TaskCache\\Tasks"
            }
          }
        }
      }
    }
  }
}