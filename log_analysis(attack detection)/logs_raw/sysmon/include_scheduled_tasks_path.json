{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "include",
          "TargetObject": [
            {
              "@name": "technique_id=T1053,technique_name=Scheduled Task",
              "@condition": "contains all",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Schedule\\TaskCache\\Tasks;Actions"
            },
            {
              "@name": "technique_id=T1053,technique_name=Scheduled Task",
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\Schedule\\TaskCache\\Tree"
            },
            {
              "@name": "technique_id=T1053,technique_name=Scheduled Task",
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\Windows NT\\CurrentVersion\\SilentProcessExit\\"
            }
          ]
        }
      }
    }
  }
}