{
  "Sysmon": {
    "@schemaversion": "4.30",
    "EventFiltering": {
      "RuleGroup": {
        "@name": "",
        "@groupRelation": "or",
        "RegistryEvent": {
          "@onmatch": "include",
          "TargetObject": [
            {
              "@name": "technique_id=T1553.004,technique_name=Install Root Certificate",
              "@condition": "begin with",
              "#text": "HKLM\\SOFTWARE\\Microsoft\\EnterpriseCertificates\\Root\\Certificates"
            },
            {
              "@name": "technique_id=T1553.004,technique_name=Install Root Certificate",
              "@condition": "contains",
              "#text": "\\Microsoft\\SystemCertificates\\Root\\Certificates"
            },
            {
              "@name": "technique_id=T1553.004,technique_name=Install Root Certificate",
              "@condition": "contains",
              "#text": "\\Microsoft\\SystemCertificates\\CA\\Certificates"
            }
          ]
        }
      }
    }
  }
}